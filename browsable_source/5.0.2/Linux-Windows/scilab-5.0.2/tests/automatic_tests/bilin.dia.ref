 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/automatic_tests/bilin_data.ref','rb');
 
s = poly(0, 's');z = poly(0, 'z');
 
w = ssrand(1, 1, 3);
 
wtf = ss2tf(w);v = [2,3,-1,4];a = v(1);b = v(2);c = v(3);d = v(4);
 
%ans = [horner(wtf, (a * z + b)/(c * z + d)),ss2tf(bilin(w, [a,b,c,d]))];
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = clean(ss2tf(bilin(bilin(w, [a,b,c,d]), [d,-b,-c,a])) - wtf);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xdel_run(winsid());
 
 
mclose(%U);
 

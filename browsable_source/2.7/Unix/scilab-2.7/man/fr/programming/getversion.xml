<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>
  <TITLE>getversion  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 2000  </DATE>
  <SHORT_DESCRIPTION name="getversion"> renvoie le numéro de version de Scilab  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>version=getversion()  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>version  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : une chaîne de caractères
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    Renvoie dans <VERB>version</VERB> le numéro de version de Scilab.
  </P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
getversion()
 ]]></EXAMPLE>
</MAN>

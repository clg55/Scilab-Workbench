<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>
  <TITLE>less  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="less (&lt;)"> comparaison, signe &quot;inférieur à&quot;  </SHORT_DESCRIPTION>
  <DESCRIPTION>
  <P>
    Symbole de comparaison logique
  </P>
 

  <DESCRIPTION_ITEM  label='&lt;&gt;'> 
    signifie &quot;différent de&quot; (identique à <VERB>~=</VERB>)
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&lt;'> 
    signifie &quot;inférieur à&quot;
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&gt;'> 
    signifie &quot;supérieur à&quot;
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&lt;='> 
    signifie &quot;inférieur ou égal à&quot;
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&gt;='> 
    signifie &quot;supérieur ou égal à&quot;
  </DESCRIPTION_ITEM>
 
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>if</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

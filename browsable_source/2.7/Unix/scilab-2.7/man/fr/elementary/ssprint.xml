<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>ssprint  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="ssprint"> pretty print for linear system  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>ssprint(sl [,out])  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>sl  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : list (<VERB>syslin</VERB> list)
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>out  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : output (default value <VERB>out=%io(2)</VERB>)
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    pretty print of a linear system in state-space form
    <VERB>sl=(A,B,C,D) syslin</VERB> list.
  </P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
 a=[1 1;0 1];b=[0 1;1 0];c=[1,1];d=[3,2];
 ssprint(syslin('c',a,b,c,d))
 ssprint(syslin('d',a,b,c,d))
 ]]></EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>texprint</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>mtlb_save  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="mtlb_save"> save variables on file with matlab4 format.  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>mtlb_save fname  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>mtlb_save fname X  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>mtlb_save fname X Y Z   </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>mtlb_save fname X Y Z  -ascii  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>mtlb_save fname X Y Z  -ascii -double   </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>mtlb_save fname X Y Z  -ascii -double -tabs  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>fname  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : a file name
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>X Y Z  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : variable names
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    mtlb_save   save variables on file with matlab4 formats.
  </P>
 

  <DESCRIPTION_ITEM  label='mtlb_save fname saves all the current scilab variables which'> 
    have corresponding matlab type to the binary &quot;MAT-file&quot; named <VERB>fname.mat</VERB>. The data may be retrieved with <VERB>mtlb_load</VERB>.
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='mtlb_save fname X '> 
    saves only variable <VERB>X</VERB>.
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='mtlb_save fname X Y Z'> 
      saves variables <VERB>X</VERB>, <VERB>Y</VERB>, and <VERB>Z</VERB>.
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='mtlb_save fname X Y Z  -ascii'> 
    uses 8-digit ASCII form instead of binary.
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='mtlb_save fname X Y Z  -ascii -double'> 
    uses 16-digit ASCII form.
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='mtlb_save fname X Y Z  -ascii -double -tabs'> 
    delimits with tabs.
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;stdio&quot; value for  fname doesnt redirect save to standard output.'> 
  </DESCRIPTION_ITEM>
 
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>mtlb_load</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>save</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>load</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

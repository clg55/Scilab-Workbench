<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>whos  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>Jan 1997  </DATE>
  <SHORT_DESCRIPTION name="whos"> listing of variables in long form  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>whos()  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>whos -type typ  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>whos -name nam  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>typ  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : name of selected variable type (see typeof)
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>nam  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : first characters of selected names
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    <VERB>whos()</VERB> displays all current variable names, types and memory used 
  </P>
  <P>
    <VERB>whos -type typ </VERB> displays all current variables with specified type
  </P>
  <P>
    <VERB>whos -name nam </VERB> displays all current variables whose names begin with <VERB>nam</VERB>
  </P>
  <P>
  </P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
whos()

whos -type boolean

whos -name %

 ]]></EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>who</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>typeof</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

  <SHORT_DESCRIPTION name="x_matrix"> Xwindow editing of matrix   </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>[result]=x_matrix(label,matrix-init)   </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>label  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : character string (name of matrix)
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>matrix-init  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : real matrix
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    For reading or editing a matrix .
  </P>
  </DESCRIPTION>

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>TAN  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="tan"> tangent  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>[t]=tan(x)     </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>x  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : vector or matrix
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>t  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : vector or matrix
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    The  elements  of <VERB>t</VERB>   are  the tangent of  the elements of <VERB>x</VERB>.
  </P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
x=[1,%i,-1,-%i]
tan(x)
sin(x)./cos(x)
 ]]></EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>atan</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>tanm</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>log10  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="log10"> logarithm  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>y=log10(x)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>x  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : vector or matrix
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    decimal logarithm.If <VERB>x</VERB> is a vector <VERB>log10(x)=[log10(x1),...,log10(xn)]</VERB>.
  </P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
10.^log10([1,%i,-1,-%i])
 ]]></EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>log</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>hat</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>ieee</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>with_pvm</TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>2002  </DATE>
  <SHORT_DESCRIPTION name="with_pvm"> Checks if Scilab has been built
 with the  "Parallel Virtual Machine" interface</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>r=with_pvm()   </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
  <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>r  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : a boolean
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  </PARAM_INDENT>
 </PARAM>

  <DESCRIPTION>
  <P>
    Returns <VERB>%t</VERB> if Scilab as been built  with the
    "Parallel Virtual Machine" <LINK>pvm</LINK>   interface or <VERB>%f</VERB> if not.
  </P>
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>pvm</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

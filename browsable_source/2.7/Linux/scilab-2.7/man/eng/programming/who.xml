<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>who  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="who"> listing of variables  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>who  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>who()  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>names=who(&apos;local&apos;)  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>[names,mem]=who(&apos;local&apos;)  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>names=who(&apos;global&apos;)  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>[names,mem]=who(&apos;global&apos;)  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <DESCRIPTION>
  <P>
    <VERB>who</VERB> displays current variable names.
  </P>
  <P>
    <VERB>who(&apos;local&apos;)</VERB> or <VERB>who(&apos;get&apos;)</VERB>  Returns current variable names
    and memory used in double precision worlds.
  </P>
  <P>
    <VERB>who(&apos;global&apos;)</VERB> returns global  variable names
    and memory used in double precision worlds.
  </P>
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>whos</LINK> </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM> <LINK>who_user</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>obsvss  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="obsvss"> observable part  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>[Ao,Bo,Co]=obsvss(A,B,C [,tol])  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>[slo]=obsvss(sl [,tol])  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>A,B,C,Ao,Bo,Co  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : real matrices
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>sl,slo  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : <VERB>syslin</VERB> lists
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>tol  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : real (threshold) (default value <VERB>100*%eps</VERB>)
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    <VERB>slo=(Ao,Bo,Co)</VERB> is the observable part of linear system <VERB>sl=(A,B,C)</VERB> (<VERB>syslin</VERB> list)
  </P>
  <P>
    <VERB>tol</VERB>  threshold to test controllability (see <VERB>contr</VERB>); 
    default value = <VERB>100*%eps</VERB>
  </P>
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>contr</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>contrss</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>obsv_mat</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>obs_gram</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

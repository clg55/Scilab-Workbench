<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>mese  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="mese"> maximum entropy spectral estimation  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>[sm,fr]=mese(x [,npts]);  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>x  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : Input sampled data sequence
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>npts  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : Optional parameter giving number of points of <VERB>fr</VERB> and <VERB>sm</VERB> (default is 256)
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>sm  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : Samples of spectral estimate on the frequency grid <VERB>fr</VERB>
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>fr  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : npts equally spaced frequency samples in <VERB>[0,.5)</VERB>
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    Calculate the maximum entropy spectral estimate of <VERB>x</VERB>
  </P>
  </DESCRIPTION>
  <AUTHOR>C. B.  </AUTHOR>
</MAN>

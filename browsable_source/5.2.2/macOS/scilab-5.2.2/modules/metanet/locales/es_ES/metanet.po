msgid "Metanet"
msgstr "Metanet"

msgid "&File"
msgstr "Archivo"

msgid "&Tools"
msgstr "Herramien&tas"

msgid "&Edit"
msgstr "&Editar"

msgid "&?"
msgstr "&?"

#, c-format
msgid "%s: First argument must be a vector of size greater or equal to 2.\n"
msgstr "%s: El primer argumento debe ser un vector mayor o igual a 2.\n"

#, c-format
msgid ""
"%s: Second argument must be a vector with same dimension as the first one.\n"
msgstr ""
"%s: El segundo argumento debe ser un vector de la misma dimensión que el "
"primero.\n"

#, c-format
msgid "%s: Third argument must be a vector of size greater or equal to 1.\n"
msgstr "%s: El tercer argumento debe ser un vector mayor o igual a 1.\n"

#, c-format
msgid "%s: First argument must be a vector with positive integer values.\n"
msgstr ""
"%s: El primer argumento debe ser un vector con valores enteros positivos.\n"

#, c-format
msgid "%s: Second argument must be a vector with positive integer values.\n"
msgstr ""
"%s: El segundo argumento debe ser un vector con valores enteros positivos.\n"

#, c-format
msgid "%s: Third argument must be a vector with positive integer values.\n"
msgstr ""
"%s: El tercer argumento debe ser un vector con valores enteros positivos.\n"

msgid "Running out of memory"
msgstr "Poca memoria"

#, c-format
msgid "\"%s\" is a directory"
msgstr "%s es un directorio"

#, c-format
msgid "Directory \"%s\" does not exist"
msgstr "El directorio \"%s\" no existe"

#, c-format
msgid "Graph file \"%s/%s.graph\" does not exist"
msgstr "El archivo de grafo \"%s/%s.graph\" no existe"

#, c-format
msgid "Unable to open file \"%s/%s.graph\""
msgstr "Imposible abrir el archivo \"%s/%s.graph\""

#, c-format
msgid "Bad graph file. Node \"%s\" is duplicated"
msgstr "Archivo de grafo incorrecto. El nodo \"%s\" está duplicado"

#, c-format
msgid "Bad graph file. Node \"%s\" referenced by arc \"%s\" not found"
msgstr ""
"Archivo de grafo incorrecto. El nodo \"%s\" al que hace referencia arc "
"\"%s\" no encontrado"

msgid "Scilab module metanet not installed.\n"
msgstr "El módulo metanet de Scilab no instalado.\n"

#, c-format
msgid "Bad internal node number %d"
msgstr "Nodo interno número %d incorrecto"

#, c-format
msgid "Bad internal arc number %d"
msgstr "Arc interno número %d incorrecto"

#, c-format
msgid "Unable to write file in directory %s, check access"
msgstr "Incapaz de escribir archivo en el directorio %s, revise el acceso"

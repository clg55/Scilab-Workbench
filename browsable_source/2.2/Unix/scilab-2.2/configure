SCILAB_G=g
SCILAB_GS=sh
FGETARG=fgetarg

case "$host" in
  alpha-*-*)
    # standard compilers
    CC=cc
    CC_OPTIONS=-O
    CC_LDFLAGS=
    FC='DECFORT_DEBUG=T;export DECFORT_DEBUG;f77'
    FC_OPTIONS='-O -fpe3 -switch nosqrt_recip'
    FC_LDFLAGS=
    LD=ld
    LD_LDFLAGS=
    if test "$enable_debug" = yes; then 
      CC_OPTIONS=-g
      FC_OPTIONS='-g -fpe3 -switch nosqrt_recip'
    fi
    # GNU compilers
    if test "$GCC" = yes; then
      CC=gcc
      CC_OPTIONS=-O
      if test "$enable_debug" = yes; then 
        CC_OPTIONS=-g
      fi
    fi
    if test "$G77" = yes; then
      FC=g77
      FC_OPTIONS=-O
      if test "$enable_debug" = yes; then 
        FC_OPTIONS=-g
      fi
    fi
    MAKEFILE_TARGET=Makefile.alpha
    ;;
  rs6000-*-*)
    # standard compilers
    CC=cc
    CC_OPTIONS='-Daix -DSYSV'
    CC_LDFLAGS=
    FC=f77
    FC_OPTIONS='-qcharlen=4096'
    FC_LDFLAGS=
    LD=ld
    LD_LDFLAGS=
    if test "$enable_debug" = yes; then 
      CC_OPTIONS='-g -Daix -DSYSV'
      FC_OPTIONS='-g -qcharlen=4096'
    fi
    # GNU compilers
    if test "$GCC" = yes; then
      CC=gcc
      CC_OPTIONS=-O
      if test "$enable_debug" = yes; then 
        CC_OPTIONS=-g
      fi
    fi
    if test "$G77" = yes; then
      FC=g77
      FC_OPTIONS=-O
      if test "$enable_debug" = yes; then 
        FC_OPTIONS=-g
      fi
    fi
    MAKEFILE_TARGET=Makefile.aix
    ;;
  sparc-*-sunos*)
    # standard compilers
    CC=cc
    CC_OPTIONS=-O
    CC_LDFLAGS=-Bstatic
    FC=f77
    FC_OPTIONS=-O
    FC_LDFLAGS=-Bstatic
    LD=ld
    LD_LDFLAGS=-Bstatic
    if test "$enable_debug" = yes; then 
      CC_OPTIONS=-g
      FC_OPTIONS=-g
    fi
    # GNU compilers
    if test "$GCC" = yes; then
      CC=gcc
      CC_OPTIONS=-O
      if test "$enable_debug" = yes; then 
        CC_OPTIONS=-g
      fi
    fi
    if test "$G77" = yes; then
      FC=g77
      FC_OPTIONS=-O
      if test "$enable_debug" = yes; then 
        FC_OPTIONS=-g
      fi
    fi
    MAKEFILE_TARGET=Makefile.sunos
    ;;
  sparc-*-solaris*)
    # standard compilers
    CC=cc
    CC_OPTIONS='-O -DSVR4 -DSYSV'
    CC_LDFLAGS=
    FC=f77
    FC_OPTIONS=-O
    FC_LDFLAGS=
    LD=ld
    LD_LDFLAGS=
    if test "$enable_debug" = yes; then 
      CC_OPTIONS='-g -DSVR4 -DSYSV'
      FC_OPTIONS=-g
    fi
    # GNU compilers
    if test "$GCC" = yes; then
      CC=gcc
      CC_OPTIONS=-O
      if test "$enable_debug" = yes; then 
        CC_OPTIONS=-g
      fi
    fi
    if test "$G77" = yes; then
      FC=g77
      FC_OPTIONS=-O
      if test "$enable_debug" = yes; then 
        FC_OPTIONS=-g
      fi
    fi
    MAKEFILE_TARGET=Makefile.solaris
    ;;
# Linux a.out
  *-*-linuxaout)
    CC=cc
    CC_OPTIONS='-O -Dlinux -D_POSIX_SOURCE -fwritable-strings'
    CC_LDFLAGS=-lieee
    # script shell f77 have bugs, get ours
    FC=$SCIDIR/bin/f77-linux
    FC_OPTIONS=-O
    FC_LDFLAGS=-lieee
    LD=ld
    LD_LDFLAGS=-lieee
    if test "$enable_debug" = yes; then 
      CC_OPTIONS='-g -Dlinux -D_POSIX_SOURCE -fwritable-strings'
      FC_OPTIONS=-g
    fi
    if test "$G77" = yes; then
      FC=g77
    fi
    DLDLIB='$(SCIDIR)/libs/libdld.a'
    DLD_SUBDIR=dld
    MAKEFILE_TARGET=Makefile.linux
   ;;
# Linux ELF
  *-*-linux)
    CC=cc
    CC_OPTIONS='-O -Dlinux -D_POSIX_SOURCE -fwritable-strings'
    CC_LDFLAGS=-lieee
    # script shell f77 have bugs, get ours
    FC=$SCIDIR/bin/f77-linux
    FC_OPTIONS=-O
    FC_LDFLAGS='-lieee -ldl -rdynamic'
    LD=ld
    LD_LDFLAGS=-lieee
    if test "$enable_debug" = yes; then 
      CC_OPTIONS='-g -Dlinux -D_POSIX_SOURCE -fwritable-strings'
      FC_OPTIONS=-g
    fi
    if test "$G77" = yes; then
      FC=g77
    fi
    MAKEFILE_TARGET=Makefile.linux
   ;;
  mips-*-ultrix*)
    # standard compilers, no longer supported
    CC=cc
    CC_OPTIONS=-O0
    CC_LDFLAGS=
    FC=f77
    FC_OPTIONS='-O0 -fpe1'
    FC_LDFLAGS=
    LD=ld
    LD_LDFLAGS=

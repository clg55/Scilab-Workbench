 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/automatic_tests/fun2string_data.ref','rb');
 
txt = fun2string(asinh, 'foo');
 
%ans = write(%io(2), txt, '(a)');
function t=foo(x)
  //
  //  PURPOSE
  //     element wise hyperbolic arcsinus
  //
  //  METHOD
  //     based on the formula  asinh(z) = -i asin(i z)
  //
  if type(x) ~= 1 then   error(53, 1),end,
  if isreal(x) then
    t = imag(asin(imult(x)))
  else
    t = -imult(asin(imult(x)))
  end,
endfunction
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xdel_run(winsid());
 
 
mclose(%U);
 

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>abs</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>April 1993</DATE>
  <SHORT_DESCRIPTION name="manedit">  editing a manual item</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>manedit(manitem ,[editor])  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>manitem</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: character string (usually, name of a function)</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      <PARAM_ITEM>
        <PARAM_NAME>editor</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: character string</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P><VERB>edit(manitem ,[editor])</VERB> opens the file <VERB>manitem</VERB>
    in the editor given by <VERB>editor</VERB>.</P>
    <P>
    Default editor is Emacs. This function should be customized according
    to your needs.</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
//manedit('lqg')
 ]]>
  </EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>whereis</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>edit</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

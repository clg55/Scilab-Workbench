 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/graphical_tests/iir_data.ref','rb');
 
hz = iir(3, 'bp', 'ellip', [0.15,0.25], [0.08,0.03]);
 
[hzm,fr] = frmag(hz, 256);
 
%ans = plot2d(fr', hzm');
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xtitle('Discrete IIR filter band pass  0.15<fr<0.25 ', ' ', ' ');
 
q = poly(0, 'q');//to express the result in terms of the0...
 
hzd = horner(hz, 1/q);
 
if load_ref('hzd') then   bugmes();quit;end,
 
//delay operator q=z^-1
 
xdel_run(winsid());
 
 
mclose(%U);
 

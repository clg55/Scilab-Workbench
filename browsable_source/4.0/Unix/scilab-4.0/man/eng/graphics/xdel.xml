<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>xdel</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>April 1993</DATE>
  <SHORT_DESCRIPTION name="xdel"> delete a graphics window</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>xdel([win-nums])   </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>win-nums</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: integer or integer vector</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P><VERB>xdel</VERB> deletes the graphics windows <VERB>win-nums</VERB> or the current
    graphics window if no argument is given.</P>
  </DESCRIPTION>
  <AUTHOR>J.Ph.C.  </AUTHOR>
</MAN>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>TCL_CreateSlave</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>18 November 2005</DATE>

  <SHORT_DESCRIPTION name="TCL_CreateSlave">Create a TCL slave
  interpreter</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>TCL_CreateSlave(interp)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>interp</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: character string parameter. Name of the slave tcl interpreter
          in which the operation has to be performed.</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>This routine allows to create a TCL slave interpreter.</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[
  TCL_CreateSlave('TCLinterp')
  TCL_SetVar('a','r','TCLinterp')
  TCL_ExistVar('a','TCLinterp')
  TCL_ExistVar('a')
  TCL_DeleteInterp('TCLinterp')

]]></EXAMPLE>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>TCL_SetVar</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>TCL_ExistVar</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>TCL_DeleteInterp</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHOR>Allan CORNET</AUTHOR>
</MAN>
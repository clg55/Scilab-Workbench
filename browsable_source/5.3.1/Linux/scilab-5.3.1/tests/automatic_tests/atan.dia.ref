 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/automatic_tests/atan_data.ref','rb');
 
// examples with the second form
 
x = [1,%i,-1,%i];
 
if load_ref('x') then   bugmes();quit;end,
 
 
phasex = atan(imag(x), real(x));
 
if load_ref('phasex') then   bugmes();quit;end,
 
 
%ans = atan(0, -1);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = atan(-%eps, -1);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
 
// branch cuts
 
%ans = atan(-%eps + 2 * %i);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = atan(%eps + 2 * %i);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = atan(-%eps - 2 * %i);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = atan(%eps - 2 * %i);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
 
// values at the branching points
 
%ans = ieee(2);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = atan(%i);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = atan(-%i);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xdel_run(winsid());
 
 
mclose(%U);
 

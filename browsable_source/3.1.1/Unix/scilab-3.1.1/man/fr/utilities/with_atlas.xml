<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>

  <TITLE>with_atlas</TITLE>

  <TYPE>Fonction Scilab</TYPE>

  <DATE>2005</DATE>

  <SHORT_DESCRIPTION name="with_atlas">Test si Scilab a ete construit avec la
  librairie Atlas</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>r=with_atlas()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>r</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: un booleen</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>retourne <VERB>%t</VERB> si Scilab a ete construit avec la librairie
    Atlas ou <VERB>%f</VERB> si ce n'est pas le cas.</P>
  </DESCRIPTION>
</MAN>
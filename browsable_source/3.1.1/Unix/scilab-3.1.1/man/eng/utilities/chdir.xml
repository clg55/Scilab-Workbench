<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>chdir</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>April 1993</DATE>
  <SHORT_DESCRIPTION name="chdir"> changes Scilab current directory</SHORT_DESCRIPTION>
  <SHORT_DESCRIPTION name="cd"> changes Scilab current directory</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>chdir(path)  </CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>realpath=cd(path)  </CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>cd path  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
     <PARAM_ITEM>
        <PARAM_NAME>path</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: a character string</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>

     <PARAM_ITEM>
        <PARAM_NAME>realpath</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: a character string, the real path name after pathname
	    conversion (see below) </SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <SP>
      Change the current Scilab directory to those given by <VERB>path</VERB>. Note that path conversion is
      performed and for example <VERB>SCI/macros/util</VERB> is a valid 
      pattern for both unix and windows.
    </SP>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
chdir(TMPDIR);
pwd
cd 
cd SCI
 ]]>
  </EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>getcwd</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

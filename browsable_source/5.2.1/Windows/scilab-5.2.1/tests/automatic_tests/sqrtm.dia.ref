 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/automatic_tests/sqrtm_data.ref','rb');
 
x = [0,1;2,4];
 
if load_ref('x') then   bugmes();quit;end,
 
 
w = sqrtm(x);
 
%ans = norm(w * w - x);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
x(1, 2) = %i;
 
w = sqrtm(x);%ans = norm(w * w - x, 1);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xdel_run(winsid());
 
 
mclose(%U);
 

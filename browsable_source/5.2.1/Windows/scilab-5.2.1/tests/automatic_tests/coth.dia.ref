 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/automatic_tests/coth_data.ref','rb');
 
x = [1,2 * %i];
 
if load_ref('x') then   bugmes();quit;end,
 
 
t = exp(x);
 
%ans = (t - ones(x)0./ t)0.\ (t + ones(x)0./ t);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = coth(x);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xdel_run(winsid());
 
 
mclose(%U);
 

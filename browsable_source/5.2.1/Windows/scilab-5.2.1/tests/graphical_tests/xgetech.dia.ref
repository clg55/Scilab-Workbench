 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/graphical_tests/xgetech_data.ref','rb');
 
// first subwindow
 
%ans = xsetech([0,0,1,0.5]);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = plot2d();

Demo of plot2d()
========================================
x=(0:0.1:2*%pi)';plot2d(x,[sin(x),sin(2*x),sin(3*x)],style=[-1,-2,3],rect=[0,-2,2*%pi,2]);

 
if load_ref('%ans') then   bugmes();quit;end,
 
 
// then xsetech is used to set the second sub window
 
%ans = xsetech([0,0.5,1,0.5]);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = grayplot();

Demo of grayplot()
========================================
t=-%pi:0.1:%pi;m=sin(t)'*cos(t);grayplot(t,t,m);

 
if load_ref('%ans') then   bugmes();quit;end,
 
 
// get the graphic scales of first subwindow
 
%ans = xsetech([0,0,1,0.5]);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
[wrect,frect,logflag,arect] = xgetech();
 
// get the graphic scales of second subwindow
 
%ans = xsetech([0,0.5,1,0.5]);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
[wrect,frect,logflag,arect] = xgetech();
 
xbasc_run();
 
%ans = xset('default');
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xdel_run(winsid());
 
 
mclose(%U);
 

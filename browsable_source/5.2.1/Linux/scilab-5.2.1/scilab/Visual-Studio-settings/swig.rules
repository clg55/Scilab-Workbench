<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Swig"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="swig rule"
			DisplayName="swig rule"
			CommandLine="&quot;$(SolutionDir)tools\swig\swig.exe&quot; -java -package org.scilab.modules.$(ProjectName) -outdir &quot;$(ProjectDir)../java/org/scilab/modules/$(ProjectName)&quot;  &quot;$(InputDir)$(InputFileName)&quot;"
			Outputs="&quot;$(InputDir)$(InputName)_wrap.c&quot;"
			FileExtensions="*.i"
			ExecutionDescription="Swig process of $(InputName).i ..."
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>

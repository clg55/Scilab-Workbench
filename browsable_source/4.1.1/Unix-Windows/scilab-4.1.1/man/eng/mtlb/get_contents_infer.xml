<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>get_contents_infer</TITLE>
  <TYPE>M2SCI Function</TYPE>
  <DATE>Octobre 2004</DATE>

  <SHORT_DESCRIPTION name="get_contents_infer">Search for informations
  in a "M2SCi tlist" contents</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>[infer,pos]=get_contents_infer(m2scitlist,index)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>m2scitlist</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: a "M2SCI tlist"</SP>
       </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      <PARAM_ITEM>
        <PARAM_NAME>index</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: an index similar to indexes returned by <VERB>macr2tree</VERB>.</SP>
       </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      <PARAM_ITEM>
        <PARAM_NAME>infer</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: an "infer" tlist</SP>
       </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      <PARAM_ITEM>
        <PARAM_NAME>pos</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: position of information in list</SP>
       </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>
    This functions searches for inference informations of a given
    index in the contents of a Cell or a Struct taken in account the
    <VERB>*</VERB>. If no information has been found, returned values
    are infer=infer() and pos=0.
    </P>  
  </DESCRIPTION>

  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>Infer</LINK> </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM> <LINK>Contents</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHORS>
    <AUTHORS_ITEM>V.C.</AUTHORS_ITEM>
  </AUTHORS>

</MAN>


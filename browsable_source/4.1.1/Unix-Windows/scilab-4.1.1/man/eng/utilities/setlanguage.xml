<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>setlanguage</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>July 2004</DATE>

  <SHORT_DESCRIPTION name="setlanguage">Sets the internal LANGUAGE
  value.</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>setlanguage(language)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>language</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: with language='fr' or 'eng'</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P><VERB>setlanguage(language)</VERB> changes current language in scilab
    (help and menus).</P>
  </DESCRIPTION>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>LANGUAGE</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHORS>
    <AUTHORS_ITEM>A.C.</AUTHORS_ITEM>
  </AUTHORS>
</MAN>
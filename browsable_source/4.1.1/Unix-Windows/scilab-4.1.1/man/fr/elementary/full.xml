<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>full</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>January 1995</DATE>
  <SHORT_DESCRIPTION name="full"> sparse to full matrix conversion</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>X=full(sp)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>sp</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: real or complex sparse (or full) matrix</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
      <PARAM_ITEM>
        <PARAM_NAME>X</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: full matrix</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P><VERB>X=full(sp)</VERB> converts the sparse matrix <VERB>sp</VERB> into its
    full representation. (If <VERB>sp</VERB> is already full then <VERB>X</VERB> equals
    <VERB>sp</VERB>).</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
sp=sparse([1,2;5,4;3,1],[1,2,3]);
A=full(sp)
 ]]>
  </EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>sparse</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>sprand</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>speye</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

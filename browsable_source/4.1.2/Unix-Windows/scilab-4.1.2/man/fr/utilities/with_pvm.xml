<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
	<LANGUAGE>fr</LANGUAGE>
	<TITLE>with_pvm</TITLE>
	<TYPE>Scilab Function</TYPE>
	<DATE>2002</DATE>
	<SHORT_DESCRIPTION name="with_pvm">Test si Scilab a été compilé avec l'interface &quot;Parallel Virtual Machine&quot;</SHORT_DESCRIPTION>
	
	<CALLING_SEQUENCE>
		<CALLING_SEQUENCE_ITEM>r=with_pvm()   </CALLING_SEQUENCE_ITEM>
	</CALLING_SEQUENCE>
	<PARAM>
		<PARAM_INDENT>
			<PARAM_ITEM>
				<PARAM_NAME>r</PARAM_NAME>
				<PARAM_DESCRIPTION>
					<SP>: a boolean</SP>
				</PARAM_DESCRIPTION>
			</PARAM_ITEM>
		</PARAM_INDENT>
	</PARAM>
	<DESCRIPTION>
		<P>
			Retourne <VERB>%t</VERB> si Scilab a été compilé avec l'interface &quot;Parallel Virtual Machine&quot; ou <VERB>%f</VERB> sinon.
		</P>
	</DESCRIPTION>
</MAN>

 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/graphical_tests/isoview_data.ref','rb');
 
t = (0:0.1:2 * %pi)';
 
%ans = plot2d(sin(t), cos(t));
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = xbasc_run();
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = isoview(-1, 1, -1, 1);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = plot2d(sin(t), cos(t), 1, '001');
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = xset('default');
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
 
%ans = plot2d(sin(t), cos(t), frameflag=4);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xdel_run(winsid());
 
 
mclose(%U);
 

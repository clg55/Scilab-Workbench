// LIST OR TLIST CASE
 
l = list(1, 'qwerw', %s);
 
if load_ref('l') then   bugmes();quit;end,
 
 
%ans = l(1);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
[a,b] = l([3,2]);
 
if load_ref('b') then   bugmes();quit;end,
 
if load_ref('a') then   bugmes();quit;end,
 
 
%ans = l($);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
x = tlist(l(2:3));
 
if load_ref('x') then   bugmes();quit;end,
 
//form a tlist with the last 2 components of l
 
//
 
dts = list(1, tlist(['x';'a';'b'], 10, [2,3]));
 
%ans = dts(2).a;
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
%ans = dts(2).b(1, 2);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
[a,b] = dts(2)(['a','b']);
 
if load_ref('b') then   bugmes();quit;end,
 
if load_ref('a') then   bugmes();quit;end,
 
 
 
xdel_run(winsid());
 
 
mclose(%U);
 

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>
  <TITLE>polfact  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="polfact"> facteurs minimaux  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>[f]=polfact(p)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
	<PARAM_NAME>p  </PARAM_NAME>
	<PARAM_DESCRIPTION>
	  <SP>
	    : polynôme
	  </SP>
	</PARAM_DESCRIPTION> 
      </PARAM_ITEM>
      <PARAM_ITEM>
	<PARAM_NAME>f  </PARAM_NAME>
	<PARAM_DESCRIPTION>
	  <SP>
	    : vecteur <VERB>[f0 f1 ... fn]</VERB> tel que <VERB>p=prod(f)</VERB> 
	  </SP>

	  <PARAM_INDENT>
	    <PARAM_ITEM>
	      <PARAM_NAME>f0  </PARAM_NAME>
	      <PARAM_DESCRIPTION>
		<SP>
		  : constante
		</SP>
	      </PARAM_DESCRIPTION> 
	    </PARAM_ITEM>
	    <PARAM_ITEM>
	      <PARAM_NAME>fi  </PARAM_NAME>
	      <PARAM_DESCRIPTION>
		<SP>
		  : polynôme
		</SP>
	      </PARAM_DESCRIPTION> 
	    </PARAM_ITEM>

	  </PARAM_INDENT>
	</PARAM_DESCRIPTION> 
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P>
      <VERB>f=polfact(p)</VERB> renvoie les facteurs minimaux de p
      i.e. <VERB>f=[f0 f1 ... fn]</VERB> tel que <VERB>p=prod(f)</VERB>
    </P>
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>lcm</LINK> </SEE_ALSO_ITEM> 
    <SEE_ALSO_ITEM> <LINK>cmndred</LINK> </SEE_ALSO_ITEM> 
    <SEE_ALSO_ITEM> <LINK>factors</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
  <AUTHOR> S. Steer INRIA </AUTHOR> 
</MAN>

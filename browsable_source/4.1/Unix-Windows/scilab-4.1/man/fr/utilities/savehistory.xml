<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>

  <TITLE>savehistory</TITLE>

  <TYPE>Fonction Scilab</TYPE>

  <DATE>17-July-2004</DATE>

  <SHORT_DESCRIPTION name="savehistory">sauvegarde l'historique de scilab dans un fichier.</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>savehistory()</CALLING_SEQUENCE_ITEM>

    <CALLING_SEQUENCE_ITEM>savehistory(f)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>f</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: chemin du fichier dans lequel sera sauvegardé l'historique de scilab</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>sauvegarde l'historique de scilab dans un fichier.</P>

    <P>Par défaut, l'historique est sauvegardé dans home+'/history.scilab'</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[savehistory(SCI+'/session.scilab')]]></EXAMPLE>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>loadhistory</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>resethistory</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>gethistory</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHORS>
    <AUTHORS_ITEM>A.C</AUTHORS_ITEM>
  </AUTHORS>
</MAN>
<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>

  <TITLE>loadhistory</TITLE>

  <TYPE>Fonction Scilab</TYPE>

  <DATE>17-July-2004</DATE>

  <SHORT_DESCRIPTION name="loadhistory">charge un fichier contenant l'historique</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>loadhistory()</CALLING_SEQUENCE_ITEM>

    <CALLING_SEQUENCE_ITEM>loadhistory(f)</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>f</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: chemin du fichier à charger</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>charge un fichier contenant l'historique</P>

    <P>Par défaut, le nom du fichier contenant l'historique est home+'/history.scilab'</P>
  </DESCRIPTION>

  <EXAMPLE><![CDATA[loadhistory(SCI+'/session.scilab')]]></EXAMPLE>

  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>savehistory</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>resethistory</LINK>
    </SEE_ALSO_ITEM>

    <SEE_ALSO_ITEM>
      <LINK>gethistory</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>

  <AUTHORS>
    <AUTHORS_ITEM>A.C</AUTHORS_ITEM>
  </AUTHORS>
</MAN>
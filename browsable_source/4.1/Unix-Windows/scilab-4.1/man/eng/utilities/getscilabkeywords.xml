<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>

  <TITLE>getscilabkeywords</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>Octobre 2005</DATE>

  <SHORT_DESCRIPTION name="getscilabkeywords">returns a list with all scilab
  keywords.</SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>list_keywords=getscilabkeywords()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>list_keywords</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <SP>: a list</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>list_keywords(1) : primitives</P>

    <P>list_keywords(2) : commands</P>

    <P>list_keywords(3) : predef variables</P>

    <P>list_keywords(4) :scilab functions</P>

    <P>list_keywords(5) :scicos functions</P>
  </DESCRIPTION>

  <AUTHOR>A.C</AUTHOR>
</MAN>
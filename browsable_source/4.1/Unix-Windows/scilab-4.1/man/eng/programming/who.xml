<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>who</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>April 1993</DATE>
  <SHORT_DESCRIPTION name="who"> listing of variables</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>who</CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>who()  </CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>names=who('local')  </CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>[names,mem]=who('local')  </CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>names=who('global')  </CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>[names,mem]=who('global')  </CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>  </CALLING_SEQUENCE_ITEM>
    <CALLING_SEQUENCE_ITEM>  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <DESCRIPTION>
    <P><VERB>who</VERB> displays current variable names.</P>
    <P><VERB>who('local')</VERB> or <VERB>who('get')</VERB>  Returns current variable names
    and memory used in double precision worlds.</P>
    <P><VERB>who('global')</VERB> returns global  variable names
    and memory used in double precision worlds.</P>
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>whos</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>who_user</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

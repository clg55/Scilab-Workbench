<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>getpid</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>April 1993</DATE>
  <SHORT_DESCRIPTION name="getpid"> get Scilab process identificator</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>id=getpid()  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <DESCRIPTION>
    <P>
    Return an the scilab process identificator integer</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
d='SD_'+string(getpid())+'_'
 ]]>
  </EXAMPLE>
</MAN>

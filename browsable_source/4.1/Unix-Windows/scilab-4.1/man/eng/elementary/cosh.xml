<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>cosh</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>April 1993</DATE>
  <SHORT_DESCRIPTION name="cosh"> hyperbolic cosine</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>[t]=cosh(x)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>x,t</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: real or complex vectors/matrices</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P>
    The  elements  of <VERB>t</VERB> are  the  hyperbolic  cosine   
    of  the corresponding entries of vector <VERB>x</VERB>.</P>
  </DESCRIPTION>
  <EXAMPLE>
<![CDATA[
x=[0,1,%i]
acosh(cosh(x))
 ]]>
  </EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>cos</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>acosh</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

  <TITLE>gethistory</TITLE>

  <TYPE>Scilab Function</TYPE>

  <DATE>17-July-2004</DATE>

  <SHORT_DESCRIPTION name="gethistory">returns current scilab history in a
  string matrix </SHORT_DESCRIPTION>

  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>matstr=gethistory()</CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>

  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>matstr</PARAM_NAME>

        <PARAM_DESCRIPTION>
          <P>a string matrix</P>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>

  <DESCRIPTION>
    <P>returns current scilab history in a string matrix .</P>
  </DESCRIPTION>


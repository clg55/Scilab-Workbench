<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>alufunctions</TITLE>
  <TYPE>Scilab keyword</TYPE>
  <DATE>April 1993</DATE>
  <SHORT_DESCRIPTION name="alufunctions">pixel drawing functions</SHORT_DESCRIPTION>
  <DESCRIPTION>
    <P>
    src is the source ie the &quot;value of the pixel&quot; which we want to draw.
    dst is the destination ie &quot;value of the pixel&quot; which is already drawn.</P>
    <ITEMIZE>
      <ITEM label="0">
        <SP>: clear ie &quot;0&quot;</SP>
      </ITEM>
      <ITEM label="1">
        <SP>: and ie &quot;src AND dst&quot;</SP>
      </ITEM>
      <ITEM label="2">
        <SP>: and reverse ie &quot;src AND NOT dst&quot;</SP>
      </ITEM>
      <ITEM label="3">
        <SP>: copy ie &quot;src&quot;</SP>
      </ITEM>
      <ITEM label="4">
        <SP>: and inverted ie &quot;(NOT src) AND dst&quot;</SP>
      </ITEM>
      <ITEM label="5">
        <SP>: noop ie &quot;dst&quot;</SP>
      </ITEM>
      <ITEM label="6">
        <SP>: xor ie &quot;src XOR dst&quot;</SP>
      </ITEM>
      <ITEM label="7">
        <SP>: or ie &quot;src OR dst&quot;</SP>
      </ITEM>
      <ITEM label="8">
        <SP>: nor ie &quot;(NOT src) AND (NOT dst)&quot;</SP>
      </ITEM>
      <ITEM label="9">
        <SP>: equiv ie &quot;(NOT src) XOR dst&quot;</SP>
      </ITEM>
      <ITEM label="10">
        <SP>: invert ie &quot;NOT dst&quot;</SP>
      </ITEM>
      <ITEM label="11">
        <SP>: or reverse ie &quot;src OR (NOT dst)&quot;</SP>
      </ITEM>
      <ITEM label="12">
        <SP>: copy inverted ie &quot;NOT src&quot;</SP>
      </ITEM>
      <ITEM label="13">
        <SP>: or inverted ie &quot;(NOT src) OR dst&quot;</SP>
      </ITEM>
      <ITEM label="14">
        <SP>: nand ie &quot;(NOT src) OR (NOT dst)&quot;</SP>
      </ITEM>
      <ITEM label="15">
        <SP>: set ie &quot;1&quot;</SP>
      </ITEM>
    </ITEMIZE>
  </DESCRIPTION>
</MAN>

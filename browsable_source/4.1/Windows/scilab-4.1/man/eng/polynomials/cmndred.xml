<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE MAN SYSTEM "../../manrev.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>cmndred</TITLE>
  <TYPE>Scilab Function</TYPE>
  <DATE>April 1993</DATE>
  <SHORT_DESCRIPTION name="cmndred"> common denominator form</SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
    <CALLING_SEQUENCE_ITEM>[n,d]=cmndred(num,den)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
    <PARAM_INDENT>
      <PARAM_ITEM>
        <PARAM_NAME>num, den</PARAM_NAME>
        <PARAM_DESCRIPTION>
          <SP>: two polynomial matrices of same dimensions</SP>
        </PARAM_DESCRIPTION>
      </PARAM_ITEM>
    </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
    <P><VERB>[n,d]=cmndred(num,den)</VERB> computes a polynomial matrix
    <VERB>n</VERB> and a common denominator polynomial <VERB>d</VERB>
    such that:</P>
    <P>
      <VERB>n/d=num./den</VERB>
    </P>
    <P>
    The rational matrix defined by <VERB>num./den</VERB> is <VERB>n/d</VERB></P>
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM>
      <LINK>simp</LINK>
    </SEE_ALSO_ITEM>
    <SEE_ALSO_ITEM>
      <LINK>clean</LINK>
    </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

 
getf SCI/util/testexamples.sci
 
reinit_for_test()
 
%U=mopen('SCI/tests/graphical_tests/g_margin_data.ref','rb');
 
h = syslin('c', -1 + %s, 3 + 2 * %s + %s^2);
 
if load_ref('h') then   bugmes();quit;end,
 
 
[g,fr] = g_margin(h);
 
if load_ref('fr') then   bugmes();quit;end,
 
if load_ref('g') then   bugmes();quit;end,
 
 
[g,fr] = g_margin(h - 10);
 
if load_ref('fr') then   bugmes();quit;end,
 
if load_ref('g') then   bugmes();quit;end,
 
 
%ans = nyquist(h - 10);
 
if load_ref('%ans') then   bugmes();quit;end,
 
 
xdel_run(winsid());
 
 
mclose(%U);
 

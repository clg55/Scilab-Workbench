<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>rchisq  </TITLE>
  <TYPE>Stixbox Function  </TYPE>
  <DATE>Nov 2001  </DATE>
  <SHORT_DESCRIPTION name="rchisq"> Random numbers from the chisquare distribution  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>x = rchisq(n,d)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>n  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : positive integer or a vector [lig,col] of integers
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>d  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : degree of freedom
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>x  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : n-vector or a n-matrix of random numbers chosen from chisquare distribution with d degrees of freedom
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    Random numbers from the chisquare distribution. 
  </P>
  <P>
  </P>
  <P>
  </P>
  <P>
  </P>
  </DESCRIPTION>
</MAN>

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>fr</LANGUAGE>
  <TITLE>typeof  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="typeof">  type d&apos;un objet  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>[t]=typeof(obj)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>obj  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : objet Scilab
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>t  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : string
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    <VERB>t=typeof(obj)</VERB> renvoie une des chaînes de caractères :
  </P>
 

  <DESCRIPTION_ITEM  label='&quot;constant&quot;'> 
    si l&apos;objet est une matrice réelle ou complexe
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;polynomial&quot;'> 
    si l&apos;objet est une matrice de polynômes
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;function&quot;'> 
    si l&apos;objet est une fonction
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;string&quot;'> 
    si l&apos;objet est une matrice de chaînes de caractères
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;boolean&quot;'> 
    si l&apos;objet est une matrice booléenne
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;list&quot;'> 
    si l&apos;objet est une liste
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;rational&quot;'> 
    si l&apos;objet est une matrice de fractions rationnelles
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;state-space&quot;'> 
    si l&apos;objet est un système dynamique linéaire (voir <VERB>syslin</VERB>)
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;sparse&quot;'> 
    si l&apos;objet est une matrice creuse
  </DESCRIPTION_ITEM>

  <DESCRIPTION_ITEM  label='&quot;boolean sparse&quot;'> 
    si l&apos;objet est une matrice booléenne creuse
  </DESCRIPTION_ITEM>
 
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
typeof(1)
typeof(poly(0,'x'))
typeof(1/poly(0,'x'))
typeof(%t)
w=sprand(100,100,0.001);
typeof(w)
typeof(w==w)
deff('y=f(x)','y=2*x');
typeof(f)
 ]]></EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>type</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>strings</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>syslin</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>poly</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>tanh  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="tanh"> hyperbolic tangent  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>t=tanh(x)   </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <DESCRIPTION>
  <P>
    the elements of <VERB>t</VERB> are the hyperbolic 
    tangents  of the elements of <VERB>x</VERB>
  </P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
x=[1,%i,-1,-%i]
tanh(x)
sinh(x)./cosh(x)
 ]]></EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>atanh</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>tan</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>tanhm</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

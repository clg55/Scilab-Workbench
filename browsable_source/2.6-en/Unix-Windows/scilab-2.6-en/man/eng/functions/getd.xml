<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>getd  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1998  </DATE>
  <SHORT_DESCRIPTION name="getd"> getting all functions defined in a directory  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>getd(path)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>path  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : Scilab string. The directory pathname
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    loads all <VERB>.sci</VERB> files (containing Scilab functions) defined in
    the <VERB>path</VERB> directory. 
  </P>
  </DESCRIPTION>
  <EXAMPLE><![CDATA[
getd('SCI/macros/auto')
 ]]></EXAMPLE>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>getf</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>lib</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>getcwd</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>pwd</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>chdir</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>getio  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>december 2000  </DATE>
  <SHORT_DESCRIPTION name="getio"> get Scilab input/output logical units  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>ios=getio()  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>ios  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : a vector [rio rte wio wte]
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>rio  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : current logical unit for reading instructions
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>rte  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : logical unit assigned for input in main scilab window
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>wio  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : logical unit relative to the diary file if any. wio=0 stands for no diary file opened
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>wte  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : logical unit assigned for output in main scilab window
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    <VERB>getio</VERB> returns logical units assigned for main scilab input and output
  </P>
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>file</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>exec</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

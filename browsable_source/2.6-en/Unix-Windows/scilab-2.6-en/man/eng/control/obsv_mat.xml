<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?> 
<!DOCTYPE MAN SYSTEM "../../man.dtd">
<MAN>
  <LANGUAGE>eng</LANGUAGE>
  <TITLE>obsv_mat  </TITLE>
  <TYPE>Scilab Function  </TYPE>
  <DATE>April 1993  </DATE>
  <SHORT_DESCRIPTION name="obsv_mat"> observability matrix  </SHORT_DESCRIPTION>
  <CALLING_SEQUENCE>
  <CALLING_SEQUENCE_ITEM>[O]=obsv_mat(A,C)  </CALLING_SEQUENCE_ITEM>
  <CALLING_SEQUENCE_ITEM>[O]=obsv_mat(sl)  </CALLING_SEQUENCE_ITEM>
  </CALLING_SEQUENCE>
  <PARAM>
 <PARAM_INDENT>
  <PARAM_ITEM>
  <PARAM_NAME>A,C,O  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : real matrices
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
  <PARAM_ITEM>
  <PARAM_NAME>sl  </PARAM_NAME>
  <PARAM_DESCRIPTION>
    : <VERB>syslin</VERB> list
  </PARAM_DESCRIPTION> 
  </PARAM_ITEM>
 </PARAM_INDENT>
  </PARAM>
  <DESCRIPTION>
  <P>
    <VERB>obsv_mat</VERB> returns the observability matrix:
  </P>
  <VERBATIM><![CDATA[
O=[C; CA; CA^2;...; CA^(n-1) ]
   ]]></VERBATIM>
  </DESCRIPTION>
  <SEE_ALSO>
    <SEE_ALSO_ITEM> <LINK>contrss</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>obsvss</LINK> </SEE_ALSO_ITEM>     <SEE_ALSO_ITEM> <LINK>obs_gram</LINK> </SEE_ALSO_ITEM>
  </SEE_ALSO>
</MAN>

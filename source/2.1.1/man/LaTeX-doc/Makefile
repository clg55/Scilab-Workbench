all:: info

info:
	@echo " The manual document file is Docu.ps.Z "
	@echo " type : rm Docu.ps.Z ; make doc"
	@echo " to recreate it from scratch"

doc :Docu.ps.Z

SUBDIRS=tr2latex Test Sample

subs::
	@case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for i in $(SUBDIRS)  ;\
	do \
		(cd $$i ; echo "making" subs "in $(CURRENT_DIR)/$$i..."; \
			$(MAKE) $(MFLAGS) CDEBUGFLAGS='$(CDEBUGFLAGS)' all); \
	done

depend::
	@case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for i in $(SUBDIRS) ;\
	do \
		(cd $$i ; echo "depending" "in $(CURRENT_DIR)/$$i..."; \
			$(MAKE) $(MFLAGS)  depend); \
	done

distclean::
	@case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for i in $(SUBDIRS) ;\
	do \
		(cd $$i ; echo "making" distclean "in $(CURRENT_DIR)/$$i..."; \
			$(MAKE) $(MFLAGS)  distclean); \
	done

Docu.ps.Z : chapters
	latex Docu
	makeindex -s Docu.isty Docu
	latex Docu
	dvips -o Docu.ps Docu.dvi
	compress Docu.ps

chapters : $(FILES)

$(TR2TEX):
	cd tr2latex ; $(MAKE) $(MFLAGS)

Chap1.tex : $(TR2TEX) $(MAN1)/man1/?*.1
	$(TR2TEX)  -m $(MAN1)/man1/?*.1 > /tmp/Chap1.tex
	@sed -e 's/\\documentstyle/%\\documentstyle/' 			-e 's/\\end{document}//' -e 's/\\begin{document}//' /tmp/Chap1.tex > Chap1.tex
	$(RM) /tmp/Chap1.tex

